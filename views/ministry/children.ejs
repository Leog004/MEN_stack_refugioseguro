    <style type="text/css">
           .arrow-down{
  width: 100%;
  margin: 0 auto;
  text-align: center;
  position: absolute;
  bottom: 100px;
}

.arrow-down::after{
  content: "";
  width: 20px;
  height: 20px;
  position: absolute;
  margin:auto;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  border-right: 4px solid #fff;
  border-bottom: 4px solid #fff;
  transform:rotate(45deg);
  animation: 3s arrow infinite ease;
}


@keyframes arrow{
  0%, 100%{
    top:50px;
  }
  50%{
    top:80px;
  }
}


.slideshow { 
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: -1;
  list-style: none;
  margin: 0;
  padding: 0;
  opacity: 1;
  
}

.slideshow li{ 
 
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background-size: cover;
  background-position: 50% 50%;
  background-repeat: no-repeat;
  opacity: 0;
  z-index: 0;
  animation: imageAnimation 50s linear infinite; 
}


.slideshow li:nth-child(1) { 
  background-image: linear-gradient(to bottom, rgba(29, 67, 80, 0.9) 0, rgba(29, 67, 80, .4)  100%),url("/images/childrenheader1.jpg") 
}
.slideshow li:nth-child(2) { 
